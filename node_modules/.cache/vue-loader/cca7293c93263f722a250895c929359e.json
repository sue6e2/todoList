class=\"btnSection\">\n            <span class=\"btnEdit\" v-on:click=\"editTodo(todoItem, index, isEdit)\">✏️</span>\n            <span class=\"btnDelete\" v-on:click=\"removeTodo(todoItem, index)\">❌</span>\n          </div>\n        </div>\n      </li>\n    </ul>\n\n  </div>\n  <div>\n    <button v-on:click=\"clearTodo\">Clear All</button>\n  </div>\n</template>\n\n<script>\nexport default {\n  data: function(){\n    return {\n      newTodoItem: \"\",\n      todoItems:[],\n      isEdit : false,\n    }\n  },\n  props: ['propsdata'],\n  methods: {\n    addTodo(){\n      if(this.newTodoItem !==''){\n        var obj = {completed: false, item : this.newTodoItem && this.newTodoItem.trim()};\n        this.$emit('addTodo', obj);\n        this.newTodoItem=\"\";\n      }\n    },\n    editTodo(todoItem, index, isEdit){\n      // console.log(index);\n      // console.log(localStorage.getItem(todoItem));\n\n      if(isEdit){\n        this.isEdit = false;\n      }\n      else{\n        this.isEdit = true;\n        this.editItem = this.propsdata[index];\n        this.editAfter = localStorage.getItem(this.editItem);\n        console.log(localStorage.getItem(this.editItem));\n      }\n    },\n    editBtn(index){\n      this.isEdit = false;\n      var editedItem = this.editItem;\n      localStorage.setItem(this.editItem, this.editItem);\n      localStorage.removeItem(this.editAfter);\n      this.$emit('editBtn', index, editedItem);\n    },\n    removeTodo(todoItem, index){\n      this.$emit('removeTodo', todoItem, index);\n    },\n    clearTodo(){\n      this.$emit('removeAll');\n    }\n  }\n}\n</script>\n\n<style scoped>\n.listSection{\n  list-style: none;\n  width: 500px;\n  margin: 0 auto;\n  padding: 30px;\n}\n.Items{\n  margin: 5px;\n  border: solid 1px;\n}\n.itemSection{\n  display: flex;\n}\n.btnSection{\n  justify-content: right;\n}\n\n</style>\n"]}]}