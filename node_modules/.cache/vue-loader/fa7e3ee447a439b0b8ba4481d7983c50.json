etItem(obj2.item, obj2.isCompleted)\n      console.log(obj2);\n    },\n    editTodo(){\n      console.log(\"\");\n    },\n    editBtn(index, editedItem){\n      this.isEdit = false;\n      console.log(editedItem);\n      this.todoItems.splice(index,1);\n      this.todoItems.push(editedItem);\n    },\n    removeTodo(todoItem, index){\n      localStorage.removeItem(todoItem);\n      this.todoItems.splice(index,1);\n    },\n    clearAll(){\n      localStorage.clear();\n      this.todoItems =[];\n    }\n  },\n  components: {\n    TodoHeader,\n    TodoList,\n    TodoInput\n  }\n}\n</script>\n\n<style>\n@font-face {\n  font-family: 'SANGJUDajungdagam';\n  src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_2112@1.0/SANGJUDajungdagam.woff') format('woff');\n  font-weight: normal;\n  font-style: normal;\n}\n\n#app {\n  font-family: 'SANGJUDajungdagam';\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]}]}