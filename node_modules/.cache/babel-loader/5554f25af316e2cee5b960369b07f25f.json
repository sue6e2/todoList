I,CAPN,C,CAHF,C;GAWK,CAXL,E;;AAAA,GAYG,EAbL,CAeI,CAtBN,C,EAuBA,mBAAA,CAEM,KAFN,EAEM,IAFN,EAEM,CADJ,mBAAA,CAAiD,QAAjD,EAAiD;AAApC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,SAAA,IAAA,QAAA,CAAA,SAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAA+B,GAAjD,EAA+B,WAA/B,CACI,CAFN,C","sourcesContent":["<template>\n  <div>\n    <h1>Todo List</h1>\n  </div>\n  <div>\n    <div class =\"inputSection\">\n      <input class=\"input\" type =\"text\" v-model = \"newTodoItem\" v-on:keyup.enter=\"addTodo\">\n      <button class=\"btnAdd\" v-on:click=\"addTodo\">add</button>\n      <input class=\"input\" v-if=\"isEdit\" v-model=\"editItem\" v-on:keyup.enter=\"editBtn\"/>\n      <button class =\"btnEdit\" v-if=\"isEdit\" v-on:click=\"editBtn\">edit</button>\n    </div>\n    <ul class=\"listSection\">\n      <li class=\"Items\" v-for=\"(todoItem, index) in propsdata\"\n          :key=\"index\"\n          :isEdit=\"false\">\n        <div class=\"itemSection\">\n          <input type=\"checkbox\" v-on:click=\"check(todoItem, index, isCompleted)\" />\n          <span class =\"isCompleted ? 'checked' : ''\" v-bind=\"check\">{{ todoItem }}</span>\n          <div class=\"btnSection\">\n            <span class=\"btn\" v-on:click=\"editTodo(todoItem, index, isEdit)\">✏️</span>\n            <span class=\"btn\" v-on:click=\"removeTodo(todoItem, index)\">❌</span>\n          </div>\n        </div>\n      </li>\n    </ul>\n\n  </div>\n  <div>\n    <button v-on:click=\"clearTodo\">Clear All</button>\n  </div>\n</template>\n\n<script>\nexport default {\n  data: function(){\n    return {\n      newTodoItem: \"\",\n      todoItems:[],\n      isEdit : false,\n      editItem : \"\",\n      isCompleted: false,\n    }\n  },\n  props: ['propsdata'],\n  methods: {\n    addTodo(){\n      if(this.newTodoItem !==''){\n        var obj = {isCompleted: false, item : this.newTodoItem && this.newTodoItem.trim()};\n        this.$emit('addTodo', obj);\n        this.newTodoItem=\"\";\n      }\n    },\n    check(){\n      this.isCompleted = true;\n    },\n    editTodo(todoItem, index, isEdit){\n      // console.log(index);\n      // console.log(localStorage.getItem(todoItem));\n\n      if(isEdit){\n        this.isEdit = false;\n      }\n      else{\n        this.isEdit = true;\n        this.editItem = this.propsdata[index];\n        this.editAfter = localStorage.getItem(this.editItem);\n        console.log(localStorage.getItem(this.editItem));\n      }\n    },\n    editBtn(index){\n      this.isEdit = false;\n      var editedItem = this.editItem;\n      localStorage.setItem(this.editItem, this.editItem);\n      localStorage.removeItem(this.editAfter);\n      this.$emit('editBtn', index, editedItem);\n    },\n    removeTodo(todoItem, index){\n      this.$emit('removeTodo', todoItem, index);\n    },\n    clearTodo(){\n      this.$emit('removeAll');\n    }\n  }\n}\n</script>\n\n<style scoped>\n.inputSection{\n  position: relative;\n}\n.input{\n  width: 500px;\n  height: 32px;\n  font-size: 15px;\n  border: 0;\n  border-radius: 10px;\n  outline: none;\n  margin: 10px;\n  padding-left: 5px;\n  background-color: lightgray;\n}\n.btnAdd, .btnEdit{\n  position: relative;\n  width: 50px;\n  height: 32px;\n  right: 50px;\n  outline: none;\n}\n.listSection{\n  list-style: none;\n  width: 500px;\n  margin: 0 auto;\n  padding: 30px;\n}\n.Items{\n  padding: 10px;\n  margin: 20px;\n  border: solid 1px;\n  border-radius: 10px;\n  box-shadow: 3px 3px gray;\n}\n.itemSection{\n  display: flex;\n}\n.btnSection{\n  margin-left: auto;\n}\n.checked{\n  text-decoration: line-through;\n}\n\n.btn{\n  margin: 5px;\n}\n\n</style>\n"],"sourceRoot":""}]}