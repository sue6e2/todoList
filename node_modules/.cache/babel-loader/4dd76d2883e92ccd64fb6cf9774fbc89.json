enter=\"editBtn\"/>\n    <button id=\"editInput\" v-if=\"isEdit\" v-on:click=\"editBtn\">edit</button>\n    <ul>\n      <li v-for=\"(todoItem, index) in propsdata\"\n          :key=\"index\"\n          :isEdit=\"false\"\n      >\n        <input type=\"checkbox\" />\n        <span>{{ todoItem }}</span>\n        <span v-on:click=\"editTodo(todoItem, index, isEdit)\">수정</span>\n        <span v-on:click=\"removeTodo(todoItem, index)\">X</span>\n      </li>\n    </ul>\n\n  </div>\n  <div>\n    <button v-on:click=\"clearTodo\">Clear All</button>\n  </div>\n</template>\n\n<script>\nexport default {\n  data: function(){\n    return {\n      newTodoItem: \"\",\n      todoItems:[],\n      isEdit : false,\n    }\n  },\n  props: ['propsdata'],\n  methods: {\n    addTodo(){\n      if(this.newTodoItem !==''){\n        var obj = {completed: false, item : this.newTodoItem && this.newTodoItem.trim()};\n        this.$emit('addTodo', obj);\n        this.newTodoItem=\"\";\n      }\n    },\n    editTodo(todoItem, index, isEdit){\n      // console.log(index);\n      // console.log(localStorage.getItem(todoItem));\n\n      if(isEdit){\n        this.isEdit = false;\n      }\n      else{\n        this.isEdit = true;\n        this.editItem = this.propsdata[index];\n        this.editAfter = localStorage.getItem(this.editItem);\n        console.log(localStorage.getItem(this.editItem));\n      }\n    },\n    editBtn(index){\n      var editedItem = this.editItem\n      this.$emit('editBtn');\n    },\n    removeTodo(todoItem, index){\n      this.$emit('removeTodo', todoItem, index);\n    },\n    clearTodo(){\n      this.$emit('removeAll');\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":""}]}