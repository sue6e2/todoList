K,IAfL,EAeK;AAfD,cAAA,KAAK,EAAC,OAeL;AAdA,cAAA,GAAG,EAAE;AAcL,aAfL,E,CAGE,mBAAA,CAWM,KAXN,EAAA,UAAA,EAWM,CAVJ,mBAAA,CAA2G,OAA3G,EAA2G;AAApG,cAAA,KAAK,EAAG,UAA4F;AAAjF,cAAA,IAAI,EAAC,UAA4E;AAAhE,cAAA,EAAE,EAAA,eAAgB,KAA8C;AAAlC,cAAA,QAAM,EAAA,kBAAA,MAAA;AAAA,uBAAE,QAAA,CAAA,KAAA,CAAM,QAAN,EAAgB,KAAhB,CAAF;AAAA;AAA4B,aAA3G,E,IAAA,E;;AAAA,c,UAAA,CAUI,EATJ,mBAAA,CAIQ,OAJR,EAIQ;AAJA,cAAA,GAAG,EAAA,eAAe;AAIlB,aAJR,E,CACE,mBAAA,CAEO,MAFP,EAEO;AAFM,cAAA,EAAE,EAAA,UAAU;AAElB,aAFP,E,iBACK,Q,CADL,EACa;AAAA;AADb,cACa,UADb,C,CADF,E;;AAAA,c,UAAA,CASI,EAJJ,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,mBAAA,CAAiE,MAAjE,EAAiE;AAA3D,cAAA,KAAK,EAAC,KAAqD;AAA1C,cAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,uBAAE,QAAA,CAAA,OAAA,CAAQ,QAAR,EAAkB,KAAlB,CAAF;AAAA;AAAqC,aAAjE,EAAwD,IAAxD,EAA0D;AAAA;AAA1D,cAA0D,UAA1D,CAEI,EADJ,mBAAA,CAAmE,MAAnE,EAAmE;AAA7D,cAAA,KAAK,EAAC,KAAuD;AAA5C,cAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,uBAAE,QAAA,CAAA,UAAA,CAAW,QAAX,EAAqB,KAArB,CAAF;AAAA;AAAuC,aAAnE,EAA2D,GAA3D,EAA4D;AAAA;AAA5D,cAA4D,UAA5D,CACI,CAHN,CAII,CAXN,C,CAHF,C;WAeK,CAfL,E;;AAAA,WAAwD,EAAtC;AAAA,S,CADoB;;;;AAAA,OAApC,CAiBmB,CAjBnB;AAAA,K;;;;GADF,E;;AAAA,I,cAAA,CAoBG,CAtBL,CAwBE,EADF,mBAAA,CAAmE,QAAnE,EAAmE;AAA3D,IAAA,KAAK,EAAE,UAAoD;AAApC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,SAAA,IAAA,QAAA,CAAA,SAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAA+B,GAAnE,EAAiD,WAAjD,CACE,CAzBN,C","sourcesContent":["<template slot-scope=\"{todoItem, index}\">\n  <div class=\"container\">\n      <ul class=\"listSection\">\n        {{ message }}\n        <draggable v-model=\"todolist\" draggable=\".items\">\n          <transition-group name=\"custom-list\">\n        <li class=\"items\" v-for=\"(todoItem, index) in propsdata\"\n            :key=\"index\"\n        >\n          <div class=\"itemSection\">\n            <input class = \"checkbox\" type=\"checkbox\" :id =\"'checkList_'+index\" v-on:change=\"check(todoItem, index)\" />\n            <label :for=\"'checkList_'+index\">\n              <span v-bind:id=\"'item_'+index\">\n                {{ todoItem }}\n              </span>\n            </label>\n            <div class=\"btnSection\">\n              <span class=\"btn\" v-on:click=\"editBtn(todoItem, index)\">✏️</span>\n              <span class=\"btn\" v-on:click=\"removeTodo(todoItem, index)\">❌</span>\n            </div>\n          </div>\n        </li>\n          </transition-group>\n        </draggable>\n      </ul>\n      <button class =\"btnClear\" v-on:click=\"clearTodo\">Clear All</button>\n  </div>\n</template>\n\n<script>\nimport draggable from 'vuedraggable'\nexport default {\n  data(){\n    return {\n      newTodoItem: \"\",\n      isEdit : false,\n      isCompleted: false,\n      editItem : \"\",\n      todolist : this.propsdata\n    }\n  },\n  props: {\n    propsdata : Array,\n    message: String\n  },\n  methods: {\n    check(todoItem, index){\n      let isChecked = document.getElementById('checkList_'+index).checked;\n      if(isChecked){\n        isChecked = false;\n        let obj2 = { item : todoItem, isCompleted: true };\n        document.getElementById('item_'+ index).style.textDecoration =\"line-through\";\n        document.getElementById('item_'+ index).style.color =\"gray\";\n        this.$emit('check', obj2, index)\n      }\n      else{\n        isChecked = true\n        let obj2 = { item : todoItem, isCompleted: false }\n        document.getElementById('item_'+ index).style.textDecoration =\"none\"\n        document.getElementById('item_'+ index).style.color =\"black\"\n        this.$emit('check', obj2, index)\n      }\n      return isChecked\n    },\n    editBtn(todoItem, index){\n      if(this.isEdit){\n        this.isEdit = false\n        this.$emit('editBtn', this.isEdit, todoItem, index)\n      }\n      else{\n        this.isEdit = true\n        this.editItem = this.propsdata[index]\n        this.$emit('editBtn', this.isEdit, todoItem, index)\n      }\n    },\n    removeTodo(todoItem, index){\n      this.$emit('removeTodo', todoItem, index)\n    },\n    clearTodo(){\n      this.$emit('removeAll')\n    }\n  },\n  components:{\n    draggable\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.checkbox{\n  margin-right: 10px;\n}\n.inputSection{\n  position: relative;\n}\n.input{\n  width: 500px;\n  height: 32px;\n  font-size: 20px;\n  border: 0;\n  border-radius: 10px;\n  outline: none;\n  margin: 10px;\n  padding-left: 5px;\n  background-color: lightgray;\n  font-family: 'SANGJUDajungdagam';\n}\n.btnAdd, .btnEdit{\n  position: relative;\n  font-size: 20px;\n  right: 50px;\n  cursor: pointer;\n  align-items: center;\n  &:hover { opacity: 60% }\n}\n.listSection{\n  list-style: none;\n  width: 500px;\n  margin: 0 auto;\n  padding: 30px;\n}\n.items{\n  padding: 10px;\n  margin: 20px;\n  border: solid 1px;\n  border-radius: 10px;\n  box-shadow: 3px 3px gray;\n}\n.itemSection{\n  font-size: 20px;\n  align-items: center;\n  display: flex;\n}\n.btnSection{\n  margin-left: auto;\n}\n.checked{\n  text-decoration: line-through;\n}\n.btn{\n  margin: 5px;\n  cursor: pointer;\n  &:hover { opacity: 60% }\n}\n.btnClear{\n  border-radius: 10px;\n  width: 100px;\n  height: 50px;\n  font-size: 20px;\n  border: none;\n  cursor: pointer;\n  color: red;\n  font-family: 'SANGJUDajungdagam';\n  &:hover { width: 110px;\n    height: 55px;\n  font-size: 22px; }\n}\n.custom-list-move{\n  transition: transform 0.5s;\n}\n</style>\n"],"sourceRoot":""}]}